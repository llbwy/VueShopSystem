{"ast":null,"code":"export default {\n  data() {\n    return {\n      //控制分配权限对话框的显示与隐藏\n      //获取所有权限的数据\n      setRightDialogVisible: false,\n      rightslist: [],\n      //所有角色聊表数据\n      rolelist: []\n    };\n  },\n\n  created() {\n    this.getRolesList();\n  },\n\n  methods: {\n    async getRolesList() {\n      const {\n        data: res\n      } = await this.$http.get('roles');\n\n      if (res.meta.status !== 200) {\n        return this, $message.error('获取角色列表失败');\n      }\n\n      this.rolelist = res.data;\n    },\n\n    //根据Id删除对应的权限\n    //根据Id删除对应的权限\n    async removeRightById(role, rightId) {\n      //弹框提示用户是否删除\n      const confirmResult = await this.$confirm('此操作将永久删除该文件, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).catch(err => err);\n\n      if (confirmResult !== 'confirm') {\n        return this.$message.info('取消了删除');\n      }\n\n      const {\n        data: res\n      } = await this.$http.delete(`roles/${role.id}/rights/${rightId}`);\n\n      if (res.meta.status !== 200) {\n        return this.$message.error('删除权限失败');\n      }\n\n      role.children = res.data;\n    },\n\n    //展示分配权限对话框\n    async showSetRightDialog() {\n      //获取所有权限的数据\n      const {\n        data: res\n      } = await this.$http.get('rights/tree');\n\n      if (res.meta.status != 200) {\n        return this.$message.error('获取权限数据失败');\n      } //这是获取到的权限数据保存到data中\n\n\n      this.rightslist = res.data;\n      console.log(this.rightslist);\n      this.setRightDialogVisible = true;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+GA;EACAA;IACA;MACA;MAEA;MACAC,4BAJA;MAKAC,cALA;MAOA;MACAC;IARA;EAUA,CAZA;;EAaAC;IACA;EACA,CAfA;;EAgBAC;IACA;MACA;QAAAL;MAAA;;MACA;QACA;MACA;;MACA;IACA,CAPA;;IAOA;IACA;IACA;MACA;MACA,0CACA,oBADA,EAEA,IAFA,EAGA;QACAM,uBADA;QAEAC,sBAFA;QAGAC;MAHA,CAHA,EAQAC,KARA,CAQAC,UARA;;MASA;QACA;MACA;;MACA;QAAAV;MAAA,4BACA,oCADA;;MAGA;QACA;MACA;;MACAW;IACA,CA9BA;;IA+BA;IACA;MACA;MACA;QAAAX;MAAA;;MAEA;QACA;MACA,CANA,CAOA;;;MACA;MACAY;MACA;IACA;;EA3CA;AAhBA","names":["data","setRightDialogVisible","rightslist","rolelist","created","methods","confirmButtonText","cancelButtonText","type","catch","err","role","console"],"sourceRoot":"src/components/power","sources":["Roles.vue"],"sourcesContent":["<template>\r\n  <div id=\"main\">\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\" id=\"breadCrumb\">\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>权限管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>角色列表</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- 卡片视图 -->\r\n    <el-card>\r\n      <!-- 添加角色按钮区 -->\r\n      <el-row>\r\n        <el-col>\r\n          <el-button type=\"primary\">添加角色</el-button>\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 角色列表区 -->\r\n      <el-table :data=\"rolelist\">\r\n        <!-- 展开列 -->\r\n        <el-table-column type=\"expand\">\r\n          <template slot-scope=\"scope\">\r\n            <el-row\r\n              v-for=\"(item1, i1) in scope.row.children\"\r\n              :key=\"item1.id\"\r\n              :class=\"['bdbottom', i1 === 0 ? 'bdtop' : '', 'vcenter']\"\r\n            >\r\n              <!-- 渲染一级权限 -->\r\n              <el-col :span=\"5\">\r\n                <el-tag closable @close=\"removeRightById(scope.row, item1.id)\">\r\n                  {{ item1.authName }}</el-tag\r\n                >\r\n                <i class=\"el-icon-caret-right\"></i>\r\n              </el-col>\r\n              <!-- 渲染二级和三级权限 -->\r\n              <el-col :span=\"19\">\r\n                <!-- 通过for循环嵌套 渲染 -->\r\n                <el-row\r\n                  v-for=\"(item2, i2) in item1.children\"\r\n                  :key=\"item2.id\"\r\n                  :class=\"[i2 === 0 ? '' : 'bdtop', 'vcenter']\"\r\n                >\r\n                  <el-col :span=\"6\">\r\n                    <el-tag\r\n                      type=\"success\"\r\n                      closable\r\n                      @close=\"removeRightById(scope.row, item2.id)\"\r\n                    >\r\n                      {{ item2.authName }}\r\n                    </el-tag>\r\n                    <i class=\"el-icon-caret-right\"></i>\r\n                  </el-col>\r\n\r\n                  <el-col :span=\"18\">\r\n                    <el-tag\r\n                      type=\"warning\"\r\n                      v-for=\"item3 in item2.children\"\r\n                      :key=\"item3.id\"\r\n                      closable\r\n                      @close=\"removeRightById(scope.row, item3.id)\"\r\n                    >\r\n                      {{ item3.authName }}</el-tag\r\n                    >\r\n                  </el-col>\r\n                </el-row>\r\n              </el-col>\r\n            </el-row>\r\n            <pre>   {{ scope.row }}</pre>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <!-- 索引列 -->\r\n        <el-table-column type=\"index\"></el-table-column>\r\n        <el-table-column label=\"角色名称\" prop=\"roleName\"></el-table-column>\r\n        <el-table-column label=\"角色描述\" prop=\"roleDesc\"></el-table-column>\r\n        <el-table-column label=\"操作\" width=\"300px\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\"\r\n              >编辑\r\n            </el-button>\r\n            <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\"\r\n              >删除</el-button\r\n            >\r\n            <el-button\r\n              type=\"warning\"\r\n              icon=\"el-icon-setting\"\r\n              size=\"mini\"\r\n              @click=\"showSetRightDialog\"\r\n              >分配权限</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-card>\r\n    <!-- 分配权限 -->\r\n\r\n    <el-dialog\r\n      title=\"分配权限\"\r\n      :visible.sync=\"setRightDialogVisible\"\r\n      width=\"50%\"\r\n    >\r\n      <span>这是一段信息</span>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"setRightDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"setRightDialogVisible = false\"\r\n          >确 定</el-button\r\n        >\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      //控制分配权限对话框的显示与隐藏\r\n\r\n      //获取所有权限的数据\r\n      setRightDialogVisible: false,\r\n      rightslist: [],\r\n\r\n      //所有角色聊表数据\r\n      rolelist: [],\r\n    }\r\n  },\r\n  created() {\r\n    this.getRolesList()\r\n  },\r\n  methods: {\r\n    async getRolesList() {\r\n      const { data: res } = await this.$http.get('roles')\r\n      if (res.meta.status !== 200) {\r\n        return this, $message.error('获取角色列表失败')\r\n      }\r\n      this.rolelist = res.data\r\n    }, //根据Id删除对应的权限\r\n    //根据Id删除对应的权限\r\n    async removeRightById(role, rightId) {\r\n      //弹框提示用户是否删除\r\n      const confirmResult = await this.$confirm(\r\n        '此操作将永久删除该文件, 是否继续?',\r\n        '提示',\r\n        {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning',\r\n        }\r\n      ).catch((err) => err)\r\n      if (confirmResult !== 'confirm') {\r\n        return this.$message.info('取消了删除')\r\n      }\r\n      const { data: res } = await this.$http.delete(\r\n        `roles/${role.id}/rights/${rightId}`\r\n      )\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('删除权限失败')\r\n      }\r\n      role.children = res.data\r\n    },\r\n    //展示分配权限对话框\r\n    async showSetRightDialog() {\r\n      //获取所有权限的数据\r\n      const { data: res } = await this.$http.get('rights/tree')\r\n\r\n      if (res.meta.status != 200) {\r\n        return this.$message.error('获取权限数据失败')\r\n      }\r\n      //这是获取到的权限数据保存到data中\r\n      this.rightslist = res.data\r\n      console.log(this.rightslist)\r\n      this.setRightDialogVisible = true\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#main {\r\n  height: 100%;\r\n\r\n  #breadCrumb {\r\n    height: 15.5px;\r\n    position: absolute;\r\n    top: 80px;\r\n  }\r\n  .el-card {\r\n    position: absolute;\r\n    top: 100px;\r\n    width: 100%-11.8px;\r\n  }\r\n}\r\n.el-tag {\r\n  margin: 7px;\r\n}\r\n.bdtop {\r\n  border-top: 1px solid #eee;\r\n}\r\n.bdbottom {\r\n  border-bottom: 1px solid #eee;\r\n}\r\n.vcenter {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}