{"ast":null,"code":"import \"core-js/modules/es.error.cause.js\";\nexport default {\n  data() {\n    //验证邮箱的规则\n    var checkEmail = (rule, value, cb) => {\n      //验证邮箱的正则表达式\n      const regEmail = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(\\.[a-zA-Z0-9_-])+/;\n\n      if (regEmail.test(value)) {\n        //合法的邮箱\n        return cb();\n      }\n\n      cb(new Error('请输入合法的邮箱'));\n    }; //验证手机号的规则\n\n\n    var checkMobile = (rule, value, cb) => {\n      const regMobile = /^(0|86|17951)?(13[0-9]|15[012356789]|17[678]18[0-9]|14[57])[0-9]{8}$/;\n\n      if (regMobile.test(value)) {\n        return cb();\n      }\n\n      cb(new Error('请输入合法的手机号'));\n    }; //验证手机号的正则表达式\n\n\n    return {\n      //获取用户列表的参数对象\n      queryInfo: {\n        query: '',\n        pagenum: 1,\n        //当前页数\n        pagesize: 2 //当前每页显示多少条数据\n\n      },\n      userlist: [],\n      total: 0,\n      //控制添加用户对话框的现实与隐藏\n      addDialogVisible: false,\n      //数据总条数\n      addForm: {\n        username: '',\n        password: '',\n        email: '',\n        mobile: ''\n      },\n      //添加用户表单数据\n      addFormRules: {\n        username: [{\n          required: true,\n          message: '请输入用户邮箱',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 10,\n          message: '用户名的长度在3-10字符之间',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }, {\n          min: 6,\n          max: 15,\n          message: '用户名的长度在6-15字符之间',\n          trigger: 'blur'\n        }],\n        email: [{\n          required: true,\n          message: '请输入用户邮箱',\n          trigger: 'blur'\n        }, {\n          validator: checkEmail,\n          trigger: 'blur'\n        }],\n        mobile: [{\n          required: true,\n          message: '请输入用户手机',\n          trigger: 'blur'\n        }, {\n          validator: checkMobile,\n          trigger: 'blur'\n        }] //添加表单验证规则\n\n      }\n    };\n  },\n\n  created() {\n    this.getUserList();\n  },\n\n  methods: {\n    //点击按钮添加新用户\n    addUser() {\n      this.$refs.addFormRef.validate(async valid => {\n        if (!valid) return; //可以发起添加用户的网络请求\n\n        this.$http.post('users', this.addForm);\n      });\n    },\n\n    //监听添加用户对话的关闭事件\n    addDialogClosed() {\n      this.$refs.addFormRef.resetFields();\n    },\n\n    async getUserList() {\n      const {\n        data: res\n      } = await this.$http.get('users', {\n        params: this.queryInfo\n      });\n      if (res.meta.status !== 200) return this.$message.error('获取用户列表失败');\n      this.userlist = res.data.users;\n      this.total = res.data.total;\n    },\n\n    //监听pagesize改变的事件\n    handleSizeChange(newSize) {\n      console.log(newSize);\n      this.queryInfo.pagesize = newSize;\n      this.getUserList();\n    },\n\n    //监听页码值改变\n    handleCurrentChange(newPage) {\n      this.queryInfo.pagenum = newPage;\n      this.getUserList();\n    },\n\n    //监听switch开关状态\n    async userStateChanged(userinfo) {\n      const {\n        data: res\n      } = await this.$http.put(`users/${userinfo.id}/state/${userinfo.mg_state}`);\n\n      if (res.meta.status !== 200) {\n        userinfo.mg_state = !userinfo.mg_state;\n        return this.$message.error('更新用化状态失败');\n      }\n\n      this.$message.success('更新用户状态成功'); //添加进服务器\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAiIA;EACAA;IACA;IACA;MACA;MACA;;MACA;QACA;QACA;MACA;;MACAC;IACA,CARA,CAFA,CAWA;;;IACA;MACA,kBACA,sEADA;;MAEA;QACA;MACA;;MACAA;IACA,CAPA,CAZA,CAoBA;;;IAEA;MACA;MACAC;QACAC,SADA;QAEAC,UAFA;QAGA;QACAC,WAJA,CAKA;;MALA,CAFA;MASAC,YATA;MAUAC,QAVA;MAWA;MAEAC,uBAbA;MAcA;MACAC;QAAAC;QAAAC;QAAAC;QAAAC;MAAA,CAfA;MAgBA;MACAC;QACAJ,WACA;UACAK,cADA;UAEAC,kBAFA;UAGAC;QAHA,CADA,EAMA;UACAC,MADA;UAEAC,OAFA;UAGAH,0BAHA;UAIAC;QAJA,CANA,CADA;QAcAN,WACA;UACAI,cADA;UAEAC,gBAFA;UAGAC;QAHA,CADA,EAMA;UACAC,MADA;UAEAC,OAFA;UAGAH,0BAHA;UAIAC;QAJA,CANA,CAdA;QA2BAL,QACA;UACAG,cADA;UAEAC,kBAFA;UAGAC;QAHA,CADA,EAMA;UAAAG;UAAAH;QAAA,CANA,CA3BA;QAmCAJ,SACA;UACAE,cADA;UAEAC,kBAFA;UAGAC;QAHA,CADA,EAMA;UAAAG;UAAAH;QAAA,CANA,CAnCA,CA2CA;;MA3CA;IAjBA;EA+DA,CAtFA;;EAuFAI;IACA;EACA,CAzFA;;EA0FAC;IACA;IACAC;MACA;QACA,mBADA,CAEA;;QACA;MACA,CAJA;IAKA,CARA;;IASA;IACAC;MACA;IACA,CAZA;;IAcA;MACA;QAAAxB;MAAA;QACAyB;MADA;MAIA,6BACA;MACA;MACA;IACA,CAvBA;;IAwBA;IACAC;MACAC;MACA;MACA;IACA,CA7BA;;IA8BA;IACAC;MACA;MACA;IACA,CAlCA;;IAmCA;IACA;MACA;QAAA5B;MAAA,yBACA,iDADA;;MAGA;QACA6B;QACA;MACA;;MACA,kCARA,CASA;IACA;;EA9CA;AA1FA","names":["data","cb","queryInfo","query","pagenum","pagesize","userlist","total","addDialogVisible","addForm","username","password","email","mobile","addFormRules","required","message","trigger","min","max","validator","created","methods","addUser","addDialogClosed","params","handleSizeChange","console","handleCurrentChange","userinfo"],"sourceRoot":"src/components/user","sources":["User.vue"],"sourcesContent":["<template>\r\n  <div id=\"main\">\r\n    <div id=\"breadCrumb\">\r\n      <!-- 面包屑导航 -->\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n        <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item>用户管理</el-breadcrumb-item>\r\n        <el-breadcrumb-item>用户列表</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n    <!-- 卡片视图 -->\r\n\r\n    <el-card>\r\n      <!-- 搜索与添加区域 -->\r\n\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\"\r\n          ><el-input\r\n            placeholder=\"请输入内容\"\r\n            v-model=\"queryInfo.query\"\r\n            clearable\r\n            @clear=\"getUserList\"\r\n          >\r\n            <el-button\r\n              slot=\"append\"\r\n              icon=\"el-icon-search\"\r\n              @click=\"getUserList\"\r\n            ></el-button> </el-input\r\n        ></el-col>\r\n        <el-col :span=\"4\">\r\n          <el-button type=\"primary\" @click=\"addDialogVisible = true\"\r\n            >添加用户</el-button\r\n          >\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 用户列表区 -->\r\n      <el-table :data=\"userlist\" border stripe>\r\n        <el-table-column type=\"index\" label=\"#\"></el-table-column>\r\n        <el-table-column label=\"姓名\" prop=\"username\"></el-table-column>\r\n        <el-table-column label=\"邮箱\" prop=\"email\"></el-table-column>\r\n        <el-table-column label=\"电话\" prop=\"mobile\"></el-table-column>\r\n        <el-table-column label=\"角色\" prop=\"role_name\"></el-table-column>\r\n        <el-table-column label=\"状态\">\r\n          <template slot-scope=\"scope\">\r\n            <el-switch\r\n              v-model=\"scope.row.mg_state\"\r\n              @change=\"userStateChanged(scope.row)\"\r\n            ></el-switch>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"180px\">\r\n          <template>\r\n            <!-- 修改按钮 -->\r\n            <el-button\r\n              type=\"primary\"\r\n              icon=\"el-icon-edit\"\r\n              size=\"mini\"\r\n            ></el-button>\r\n            <!-- 删除按钮 -->\r\n            <el-button\r\n              type=\"danger\"\r\n              icon=\"el-icon-delete\"\r\n              size=\"mini\"\r\n            ></el-button>\r\n            <!-- 分配角色按钮 -->\r\n            <el-tooltip\r\n              effect=\"dark\"\r\n              content=\"分配角色\"\r\n              placement=\"top\"\r\n              :enterable=\"false\"\r\n            >\r\n              <el-button\r\n                type=\"warning\"\r\n                icon=\"el-icon-setting\"\r\n                size=\"mini\"\r\n              ></el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[1, 2, 5, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager,next,jumper\"\r\n        :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n    </el-card>\r\n    <!-- 添加用户 -->\r\n    <el-dialog\r\n      title=\"添加用户\"\r\n      :visible.sync=\"addDialogVisible\"\r\n      width=\"50%\"\r\n      @close=\"addDialogClosed\"\r\n    >\r\n      <!-- 内容主体区 -->\r\n      <el-form\r\n        :model=\"addForm\"\r\n        :rules=\"addFormRules\"\r\n        ref=\"addFormRef\"\r\n        label-width=\"70px\"\r\n      >\r\n        <el-form-item label=\"用户名\" prop=\"username\">\r\n          <el-input v-model=\"addForm.username\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\" prop=\"password\">\r\n          <el-input v-model=\"addForm.password\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\" prop=\"email\">\r\n          <el-input v-model=\"addForm.email\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"手机\" prop=\"mobile\">\r\n          <el-input v-model=\"addForm.mobile\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <!-- 底部区域 -->\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addUser\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    //验证邮箱的规则\r\n    var checkEmail = (rule, value, cb) => {\r\n      //验证邮箱的正则表达式\r\n      const regEmail = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(\\.[a-zA-Z0-9_-])+/\r\n      if (regEmail.test(value)) {\r\n        //合法的邮箱\r\n        return cb()\r\n      }\r\n      cb(new Error('请输入合法的邮箱'))\r\n    }\r\n    //验证手机号的规则\r\n    var checkMobile = (rule, value, cb) => {\r\n      const regMobile =\r\n        /^(0|86|17951)?(13[0-9]|15[012356789]|17[678]18[0-9]|14[57])[0-9]{8}$/\r\n      if (regMobile.test(value)) {\r\n        return cb()\r\n      }\r\n      cb(new Error('请输入合法的手机号'))\r\n    }\r\n    //验证手机号的正则表达式\r\n\r\n    return {\r\n      //获取用户列表的参数对象\r\n      queryInfo: {\r\n        query: '',\r\n        pagenum: 1,\r\n        //当前页数\r\n        pagesize: 2,\r\n        //当前每页显示多少条数据\r\n      },\r\n      userlist: [],\r\n      total: 0,\r\n      //控制添加用户对话框的现实与隐藏\r\n\r\n      addDialogVisible: false,\r\n      //数据总条数\r\n      addForm: { username: '', password: '', email: '', mobile: '' },\r\n      //添加用户表单数据\r\n      addFormRules: {\r\n        username: [\r\n          {\r\n            required: true,\r\n            message: '请输入用户邮箱',\r\n            trigger: 'blur',\r\n          },\r\n          {\r\n            min: 3,\r\n            max: 10,\r\n            message: '用户名的长度在3-10字符之间',\r\n            trigger: 'blur',\r\n          },\r\n        ],\r\n        password: [\r\n          {\r\n            required: true,\r\n            message: '请输入密码',\r\n            trigger: 'blur',\r\n          },\r\n          {\r\n            min: 6,\r\n            max: 15,\r\n            message: '用户名的长度在6-15字符之间',\r\n            trigger: 'blur',\r\n          },\r\n        ],\r\n        email: [\r\n          {\r\n            required: true,\r\n            message: '请输入用户邮箱',\r\n            trigger: 'blur',\r\n          },\r\n          { validator: checkEmail, trigger: 'blur' },\r\n        ],\r\n        mobile: [\r\n          {\r\n            required: true,\r\n            message: '请输入用户手机',\r\n            trigger: 'blur',\r\n          },\r\n          { validator: checkMobile, trigger: 'blur' },\r\n        ],\r\n        //添加表单验证规则\r\n      },\r\n    }\r\n  },\r\n  created() {\r\n    this.getUserList()\r\n  },\r\n  methods: {\r\n    //点击按钮添加新用户\r\n    addUser() {\r\n      this.$refs.addFormRef.validate(async valid=>{\r\n        if(!valid) return\r\n        //可以发起添加用户的网络请求\r\n        this.$http.post('users',this.addForm)\r\n      })\r\n    },\r\n    //监听添加用户对话的关闭事件\r\n    addDialogClosed() {\r\n      this.$refs.addFormRef.resetFields()\r\n    },\r\n\r\n    async getUserList() {\r\n      const { data: res } = await this.$http.get('users', {\r\n        params: this.queryInfo,\r\n      })\r\n\r\n      if (res.meta.status !== 200)\r\n        return this.$message.error('获取用户列表失败')\r\n      this.userlist = res.data.users\r\n      this.total = res.data.total\r\n    },\r\n    //监听pagesize改变的事件\r\n    handleSizeChange(newSize) {\r\n      console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getUserList()\r\n    },\r\n    //监听页码值改变\r\n    handleCurrentChange(newPage) {\r\n      this.queryInfo.pagenum = newPage\r\n      this.getUserList()\r\n    },\r\n    //监听switch开关状态\r\n    async userStateChanged(userinfo) {\r\n      const { data: res } = await this.$http.put(\r\n        `users/${userinfo.id}/state/${userinfo.mg_state}`\r\n      )\r\n      if (res.meta.status !== 200) {\r\n        userinfo.mg_state = !userinfo.mg_state\r\n        return this.$message.error('更新用化状态失败')\r\n      }\r\n      this.$message.success('更新用户状态成功')\r\n      //添加进服务器\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#main {\r\n  height: 100%;\r\n\r\n  #breadCrumb {\r\n    height: 15.5px;\r\n    position: absolute;\r\n    top: 80px;\r\n  }\r\n  .el-card {\r\n    position: absolute;\r\n    top: 100px;\r\n    width: 100%-11.8px;\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}