{"ast":null,"code":"export default {\n  data() {\n    return {\n      //获取用户列表的参数对象\n      queryInfo: {\n        query: '',\n        pagenum: 1,\n        //当前页数\n        pagesize: 2\n      },\n      userlist: [],\n      total: 0 //数据总条数\n\n    };\n  },\n\n  created() {\n    this.getUserList();\n  },\n\n  methods: {\n    async getUserList() {\n      const {\n        data: res\n      } = await this.$http.get('users', {\n        params: this.queryInfo\n      });\n      if (res.meta.status !== 200) return this.$message.error('获取用户列表失败');\n      this.userlist = res.data.users;\n      this.total = res.data.total;\n    },\n\n    //监听pagesize改变的事件\n    handleSizeChange(newSize) {\n      console.log(newSize);\n    },\n\n    //监听页码值改变\n    handleCurrentChange(newpage) {\n      log(newpage);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoFA;EACAA;IACA;MACA;MACAC;QACAC,SADA;QAEAC,UAFA;QAGA;QACAC;MAJA,CAFA;MAQAC,YARA;MASAC,QATA,CAUA;;IAVA;EAYA,CAdA;;EAeAC;IACA;EACA,CAjBA;;EAkBAC;IACA;MACA;QAAAR;MAAA;QACAS;MADA;MAIA,6BACA;MACA;MACA;IACA,CAVA;;IAWA;IACAC;MACAC;IACA,CAdA;;IAeA;IACAC;MACAC;IACA;;EAlBA;AAlBA","names":["data","queryInfo","query","pagenum","pagesize","userlist","total","created","methods","params","handleSizeChange","console","handleCurrentChange","log"],"sourceRoot":"src/components/user","sources":["User.vue"],"sourcesContent":["<template>\r\n  <div id=\"main\">\r\n    <div id=\"breadCrumb\">\r\n      <!-- 面包屑导航 -->\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n        <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item>用户管理</el-breadcrumb-item>\r\n        <el-breadcrumb-item>用户列表</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n    <!-- 卡片视图 -->\r\n\r\n    <el-card>\r\n      <!-- 搜索与添加区域 -->\r\n\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\"\r\n          ><el-input placeholder=\"请输入内容\">\r\n            <el-button\r\n              slot=\"append\"\r\n              icon=\"el-icon-search\"\r\n            ></el-button> </el-input\r\n        ></el-col>\r\n        <el-col :span=\"4\">\r\n          <el-button type=\"primary\">添加用户</el-button>\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 用户列表区 -->\r\n      <el-table :data=\"userlist\" border stripe>\r\n        <el-table-column type=\"index\" label=\"#\"></el-table-column>\r\n        <el-table-column label=\"姓名\" prop=\"username\"></el-table-column>\r\n        <el-table-column label=\"邮箱\" prop=\"email\"></el-table-column>\r\n        <el-table-column label=\"电话\" prop=\"mobile\"></el-table-column>\r\n        <el-table-column label=\"角色\" prop=\"role_name\"></el-table-column>\r\n        <el-table-column label=\"状态\">\r\n          <template slot-scope=\"scope\">\r\n            <el-switch v-model=\"scope.row.mg_state\"></el-switch>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"180px\">\r\n          <template slot-scope=\"scope\">\r\n            <!-- 修改按钮 -->\r\n            <el-button\r\n              type=\"primary\"\r\n              icon=\"el-icon-edit\"\r\n              size=\"mini\"\r\n            ></el-button>\r\n            <!-- 删除按钮 -->\r\n            <el-button\r\n              type=\"danger\"\r\n              icon=\"el-icon-delete\"\r\n              size=\"mini\"\r\n            ></el-button>\r\n            <!-- 分配角色按钮 -->\r\n            <el-tooltip\r\n              effect=\"dark\"\r\n              content=\"分配角色\"\r\n              placement=\"top\"\r\n              :enterable=\"false\"\r\n            >\r\n              <el-button\r\n                type=\"warning\"\r\n                icon=\"el-icon-setting\"\r\n                size=\"mini\"\r\n              ></el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page.sync=\"queryInfo.num\"\r\n        :page-sizes=\"[1,2,5,10]\"\r\n        layout=\"prev, pager, next, jumper\"\r\n        :total=\"1000\"\r\n      >\r\n      </el-pagination>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      //获取用户列表的参数对象\r\n      queryInfo: {\r\n        query: '',\r\n        pagenum: 1,\r\n        //当前页数\r\n        pagesize: 2,\r\n      },\r\n      userlist: [],\r\n      total: 0,\r\n      //数据总条数\r\n    }\r\n  },\r\n  created() {\r\n    this.getUserList()\r\n  },\r\n  methods: {\r\n    async getUserList() {\r\n      const { data: res } = await this.$http.get('users', {\r\n        params: this.queryInfo,\r\n      })\r\n\r\n      if (res.meta.status !== 200)\r\n        return this.$message.error('获取用户列表失败')\r\n      this.userlist = res.data.users\r\n      this.total = res.data.total\r\n    },\r\n    //监听pagesize改变的事件\r\n    handleSizeChange(newSize) {\r\n      console.log(newSize)\r\n    },\r\n    //监听页码值改变\r\n    handleCurrentChange(newpage) {\r\n      log(newpage)\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#main {\r\n  height: 100%;\r\n\r\n  #breadCrumb {\r\n    height: 15.5px;\r\n    position: absolute;\r\n    top: 80px;\r\n  }\r\n  .el-card {\r\n    position: absolute;\r\n    top: 100px;\r\n    width: 81.25%;\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}