{"ast":null,"code":"import \"core-js/modules/es.error.cause.js\";\nexport default {\n  data() {\n    //验证邮箱的规则\n    var checkEmail = (rule, value, cb) => {\n      //验证邮箱的正则表达式\n      const regEmail = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(\\.[a-zA-Z0-9_-])+/;\n\n      if (regEmail.test(value)) {\n        //合法的邮箱\n        return cb();\n      }\n\n      cb(new Error('请输入合法的邮箱'));\n    }; //验证手机号的规则\n\n\n    var checkMobile = (rule, value, cb) => {\n      const regMobile = /^(0|86|17951)?(13[0-9]|15[012356789]|17[678]18[0-9]|14[57])[0-9]{8}$/;\n\n      if (regMobile.test(value)) {\n        return cb();\n      }\n\n      cb(new Error('请输入合法的手机号'));\n    }; //验证手机号的正则表达式\n\n\n    return {\n      //修改表单的验证规则对象\n      editFormRules: {\n        email: [{\n          required: true,\n          message: '请输入用户邮箱',\n          trigger: 'blur'\n        }, {\n          validator: checkEmail,\n          trigger: 'blur'\n        }],\n        email: [{\n          required: true,\n          message: '请输入用户手机',\n          trigger: 'blur'\n        }, {\n          validator: checkMobile,\n          trigger: 'blur'\n        }]\n      },\n      //查询到的用户信息对象\n      editForm: {},\n      //修改控制用户对话框的显示与隐藏\n      editDialogVisible: false,\n      //获取用户列表的参数对象\n      queryInfo: {\n        query: '',\n        pagenum: 1,\n        //当前页数\n        pagesize: 2 //当前每页显示多少条数据\n\n      },\n      userlist: [],\n      total: 0,\n      //控制添加用户对话框的现实与隐藏\n      addDialogVisible: false,\n      //数据总条数\n      addForm: {\n        username: '',\n        password: '',\n        email: '',\n        mobile: ''\n      },\n      //添加用户表单数据\n      addFormRules: {\n        username: [{\n          required: true,\n          message: '请输入用户邮箱',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 10,\n          message: '用户名的长度在3-10字符之间',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }, {\n          min: 6,\n          max: 15,\n          message: '用户名的长度在6-15字符之间',\n          trigger: 'blur'\n        }],\n        email: [{\n          required: true,\n          message: '请输入用户邮箱',\n          trigger: 'blur'\n        }, {\n          validator: checkEmail,\n          trigger: 'blur'\n        }],\n        mobile: [{\n          required: true,\n          message: '请输入用户手机',\n          trigger: 'blur'\n        }, {\n          validator: checkMobile,\n          trigger: 'blur'\n        }] //添加表单验证规则\n\n      }\n    };\n  },\n\n  created() {\n    this.getUserList();\n  },\n\n  methods: {\n    //展示编辑用户的对话框\n    async showEditDialog(id) {\n      const {\n        data: res\n      } = await this.$http.get('users/' + id);\n      if (res.meta.status != 200) return this.$message.error('查询用户信息失败'); //以上内容是后来添加的\n\n      this.editForm = res.data;\n      this.editDialogVisible = true;\n    },\n\n    //点击按钮添加新用户\n    addUser() {\n      this.$refs.addFormRef.validate(async valid => {\n        if (!valid) return; //可以发起添加用户的网络请求\n\n        const {\n          data: res\n        } = await this.$http.post('users', this.addForm);\n\n        if (res.meta.status != 201) {\n          this.$message.error('添加新用户失败');\n        }\n\n        this.$message.success('添加新用户成功'); //隐藏用户对话框\n\n        this.addDialogVisible = false; //重新获取用户列表\n\n        this.getUserList();\n      });\n    },\n\n    //监听添加用户对话的关闭事件\n    addDialogClosed() {\n      this.$refs.addFormRef.resetFields();\n    },\n\n    async getUserList() {\n      const {\n        data: res\n      } = await this.$http.get('users', {\n        params: this.queryInfo\n      });\n      if (res.meta.status !== 200) return this.$message.error('获取用户列表失败');\n      this.userlist = res.data.users;\n      this.total = res.data.total;\n    },\n\n    //监听pagesize改变的事件\n    handleSizeChange(newSize) {\n      console.log(newSize);\n      this.queryInfo.pagesize = newSize;\n      this.getUserList();\n    },\n\n    //监听页码值改变\n    handleCurrentChange(newPage) {\n      this.queryInfo.pagenum = newPage;\n      this.getUserList();\n    },\n\n    //监听switch开关状态\n    async userStateChanged(userinfo) {\n      const {\n        data: res\n      } = await this.$http.put(`users/${userinfo.id}/state/${userinfo.mg_state}`);\n\n      if (res.meta.status !== 200) {\n        userinfo.mg_state = !userinfo.mg_state;\n        return this.$message.error('更新用化状态失败');\n      }\n\n      this.$message.success('更新用户状态成功'); //添加进服务器\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA4JA;EACAA;IACA;IACA;MACA;MACA;;MACA;QACA;QACA;MACA;;MACAC;IACA,CARA,CAFA,CAWA;;;IACA;MACA,kBACA,sEADA;;MAEA;QACA;MACA;;MACAA;IACA,CAPA,CAZA,CAoBA;;;IAEA;MACA;MACAC;QACAC,QACA;UAEAC,cAFA;UAGAC,kBAHA;UAIAC;QAJA,CADA,EAQA;UAAAC;UAAAD;QAAA,CARA,CADA;QAWAH,QACA;UAEAC,cAFA;UAGAC,kBAHA;UAIAC;QAJA,CADA,EAQA;UAAAC;UAAAD;QAAA,CARA;MAXA,CAFA;MAwBA;MACAE,YAzBA;MA0BA;MACAC,wBA3BA;MA4BA;MACAC;QACAC,SADA;QAEAC,UAFA;QAGA;QACAC,WAJA,CAKA;;MALA,CA7BA;MAoCAC,YApCA;MAqCAC,QArCA;MAsCA;MAEAC,uBAxCA;MAyCA;MACAC;QAAAC;QAAAC;QAAAhB;QAAAiB;MAAA,CA1CA;MA2CA;MACAC;QACAH,WACA;UACAd,cADA;UAEAC,kBAFA;UAGAC;QAHA,CADA,EAMA;UACAgB,MADA;UAEAC,OAFA;UAGAlB,0BAHA;UAIAC;QAJA,CANA,CADA;QAcAa,WACA;UACAf,cADA;UAEAC,gBAFA;UAGAC;QAHA,CADA,EAMA;UACAgB,MADA;UAEAC,OAFA;UAGAlB,0BAHA;UAIAC;QAJA,CANA,CAdA;QA2BAH,QACA;UACAC,cADA;UAEAC,kBAFA;UAGAC;QAHA,CADA,EAMA;UAAAC;UAAAD;QAAA,CANA,CA3BA;QAmCAc,SACA;UACAhB,cADA;UAEAC,kBAFA;UAGAC;QAHA,CADA,EAMA;UAAAC;UAAAD;QAAA,CANA,CAnCA,CA2CA;;MA3CA;IA5CA;EA0FA,CAjHA;;EAkHAkB;IACA;EACA,CApHA;;EAqHAC;IACA;IACA;MACA;QAAAzB;MAAA;MACA,mEAFA,CAGA;;MACA;MACA;IACA,CARA;;IASA;IACA0B;MACA;QACA,mBADA,CAEA;;QACA;UAAA1B;QAAA;;QACA;UACA;QACA;;QACA,iCAPA,CAQA;;QACA,8BATA,CAUA;;QACA;MACA,CAZA;IAaA,CAxBA;;IAyBA;IACA2B;MACA;IACA,CA5BA;;IA8BA;MACA;QAAA3B;MAAA;QACA4B;MADA;MAIA,6BACA;MACA;MACA;IACA,CAvCA;;IAwCA;IACAC;MACAC;MACA;MACA;IACA,CA7CA;;IA8CA;IACAC;MACA;MACA;IACA,CAlDA;;IAmDA;IACA;MACA;QAAA/B;MAAA,yBACA,iDADA;;MAGA;QACAgC;QACA;MACA;;MACA,kCARA,CASA;IACA;;EA9DA;AArHA","names":["data","cb","editFormRules","email","required","message","trigger","validator","editForm","editDialogVisible","queryInfo","query","pagenum","pagesize","userlist","total","addDialogVisible","addForm","username","password","mobile","addFormRules","min","max","created","methods","addUser","addDialogClosed","params","handleSizeChange","console","handleCurrentChange","userinfo"],"sourceRoot":"src/components/user","sources":["User.vue"],"sourcesContent":["<template>\r\n  <div id=\"main\">\r\n    <div id=\"breadCrumb\">\r\n      <!-- 面包屑导航 -->\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n        <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item>用户管理</el-breadcrumb-item>\r\n        <el-breadcrumb-item>用户列表</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n    <!-- 卡片视图 -->\r\n\r\n    <el-card>\r\n      <!-- 搜索与添加区域 -->\r\n\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\"\r\n          ><el-input\r\n            placeholder=\"请输入内容\"\r\n            v-model=\"queryInfo.query\"\r\n            clearable\r\n            @clear=\"getUserList\"\r\n          >\r\n            <el-button\r\n              slot=\"append\"\r\n              icon=\"el-icon-search\"\r\n              @click=\"getUserList\"\r\n            ></el-button> </el-input\r\n        ></el-col>\r\n        <el-col :span=\"4\">\r\n          <el-button type=\"primary\" @click=\"addDialogVisible = true\"\r\n            >添加用户</el-button\r\n          >\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 用户列表区 -->\r\n      <el-table :data=\"userlist\" border stripe>\r\n        <el-table-column type=\"index\" label=\"#\"></el-table-column>\r\n        <el-table-column label=\"姓名\" prop=\"username\"></el-table-column>\r\n        <el-table-column label=\"邮箱\" prop=\"email\"></el-table-column>\r\n        <el-table-column label=\"电话\" prop=\"mobile\"></el-table-column>\r\n        <el-table-column label=\"角色\" prop=\"role_name\"></el-table-column>\r\n        <el-table-column label=\"状态\">\r\n          <template slot-scope=\"scope\">\r\n            <el-switch\r\n              v-model=\"scope.row.mg_state\"\r\n              @change=\"userStateChanged(scope.row)\"\r\n            ></el-switch>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"180px\">\r\n          <template slot-scope=\"scope\">\r\n            <!-- 修改按钮 -->\r\n            <el-button\r\n              type=\"primary\"\r\n              icon=\"el-icon-edit\"\r\n              size=\"mini\"\r\n              @click=\"showEditDialog(scope.row.id)\"\r\n            ></el-button>\r\n            <!-- 删除按钮 -->\r\n            <el-button\r\n              type=\"danger\"\r\n              icon=\"el-icon-delete\"\r\n              size=\"mini\"\r\n            ></el-button>\r\n            <!-- 分配角色按钮 -->\r\n            <el-tooltip\r\n              effect=\"dark\"\r\n              content=\"分配角色\"\r\n              placement=\"top\"\r\n              :enterable=\"false\"\r\n            >\r\n              <el-button\r\n                type=\"warning\"\r\n                icon=\"el-icon-setting\"\r\n                size=\"mini\"\r\n              ></el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[1, 2, 5, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager,next,jumper\"\r\n        :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n    </el-card>\r\n    <!-- 添加用户 -->\r\n    <el-dialog\r\n      title=\"添加用户\"\r\n      :visible.sync=\"addDialogVisible\"\r\n      width=\"50%\"\r\n      @close=\"addDialogClosed\"\r\n    >\r\n      <!-- 内容主体区 -->\r\n      <el-form\r\n        :model=\"addForm\"\r\n        :rules=\"addFormRules\"\r\n        ref=\"addFormRef\"\r\n        label-width=\"70px\"\r\n      >\r\n        <el-form-item label=\"用户名\" prop=\"username\">\r\n          <el-input v-model=\"addForm.username\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\" prop=\"password\">\r\n          <el-input v-model=\"addForm.password\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\" prop=\"email\">\r\n          <el-input v-model=\"addForm.email\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"手机\" prop=\"mobile\">\r\n          <el-input v-model=\"addForm.mobile\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <!-- 底部区域 -->\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addUser\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <!-- 修改用户的对话框 -->\r\n\r\n    <el-dialog title=\"修改用户\" :visible.sync=\"editDialogVisible\" width=\"50%\">\r\n      <el-form\r\n        :model=\"editForm\"\r\n        :rules=\"editFormRules\"\r\n        ref=\"editFormRef\"\r\n        label-width=\"70px\"\r\n      >\r\n        <el-form-item label=\"用户名\">\r\n          <el-input v-model=\"editForm.username\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\" prop=\"email\">\r\n          <el-input v-model=\"editForm.email\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"手机\" prop=\"mobile\">\r\n          <el-input v-model=\"editForm.mobile\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogVisible = false\"\r\n          >确 定</el-button\r\n        >\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    //验证邮箱的规则\r\n    var checkEmail = (rule, value, cb) => {\r\n      //验证邮箱的正则表达式\r\n      const regEmail = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(\\.[a-zA-Z0-9_-])+/\r\n      if (regEmail.test(value)) {\r\n        //合法的邮箱\r\n        return cb()\r\n      }\r\n      cb(new Error('请输入合法的邮箱'))\r\n    }\r\n    //验证手机号的规则\r\n    var checkMobile = (rule, value, cb) => {\r\n      const regMobile =\r\n        /^(0|86|17951)?(13[0-9]|15[012356789]|17[678]18[0-9]|14[57])[0-9]{8}$/\r\n      if (regMobile.test(value)) {\r\n        return cb()\r\n      }\r\n      cb(new Error('请输入合法的手机号'))\r\n    }\r\n    //验证手机号的正则表达式\r\n\r\n    return {\r\n      //修改表单的验证规则对象\r\n      editFormRules:{ \r\n        email:[\r\n          {\r\n            \r\n            required: true,\r\n            message: '请输入用户邮箱',\r\n            trigger: 'blur',\r\n          \r\n          },\r\n          {validator:checkEmail,trigger:'blur'}\r\n        ],\r\n         email:[\r\n          {\r\n            \r\n            required: true,\r\n            message: '请输入用户手机',\r\n            trigger: 'blur',\r\n          \r\n          },\r\n          {validator:checkMobile,trigger:'blur'}\r\n       , ]\r\n      },\r\n      //查询到的用户信息对象\r\n      editForm: {},\r\n      //修改控制用户对话框的显示与隐藏\r\n      editDialogVisible: false,\r\n      //获取用户列表的参数对象\r\n      queryInfo: {\r\n        query: '',\r\n        pagenum: 1,\r\n        //当前页数\r\n        pagesize: 2,\r\n        //当前每页显示多少条数据\r\n      },\r\n      userlist: [],\r\n      total: 0,\r\n      //控制添加用户对话框的现实与隐藏\r\n\r\n      addDialogVisible: false,\r\n      //数据总条数\r\n      addForm: { username: '', password: '', email: '', mobile: '' },\r\n      //添加用户表单数据\r\n      addFormRules: {\r\n        username: [\r\n          {\r\n            required: true,\r\n            message: '请输入用户邮箱',\r\n            trigger: 'blur',\r\n          },\r\n          {\r\n            min: 3,\r\n            max: 10,\r\n            message: '用户名的长度在3-10字符之间',\r\n            trigger: 'blur',\r\n          },\r\n        ],\r\n        password: [\r\n          {\r\n            required: true,\r\n            message: '请输入密码',\r\n            trigger: 'blur',\r\n          },\r\n          {\r\n            min: 6,\r\n            max: 15,\r\n            message: '用户名的长度在6-15字符之间',\r\n            trigger: 'blur',\r\n          },\r\n        ],\r\n        email: [\r\n          {\r\n            required: true,\r\n            message: '请输入用户邮箱',\r\n            trigger: 'blur',\r\n          },\r\n          { validator: checkEmail, trigger: 'blur' },\r\n        ],\r\n        mobile: [\r\n          {\r\n            required: true,\r\n            message: '请输入用户手机',\r\n            trigger: 'blur',\r\n          },\r\n          { validator: checkMobile, trigger: 'blur' },\r\n        ],\r\n        //添加表单验证规则\r\n      },\r\n    }\r\n  },\r\n  created() {\r\n    this.getUserList()\r\n  },\r\n  methods: {\r\n    //展示编辑用户的对话框\r\n    async showEditDialog(id) {\r\n      const { data: res } = await this.$http.get('users/' + id)\r\n      if (res.meta.status != 200) return this.$message.error('查询用户信息失败')\r\n      //以上内容是后来添加的\r\n      this.editForm = res.data\r\n      this.editDialogVisible = true\r\n    },\r\n    //点击按钮添加新用户\r\n    addUser() {\r\n      this.$refs.addFormRef.validate(async (valid) => {\r\n        if (!valid) return\r\n        //可以发起添加用户的网络请求\r\n        const { data: res } = await this.$http.post('users', this.addForm)\r\n        if (res.meta.status != 201) {\r\n          this.$message.error('添加新用户失败')\r\n        }\r\n        this.$message.success('添加新用户成功')\r\n        //隐藏用户对话框\r\n        this.addDialogVisible = false\r\n        //重新获取用户列表\r\n        this.getUserList()\r\n      })\r\n    },\r\n    //监听添加用户对话的关闭事件\r\n    addDialogClosed() {\r\n      this.$refs.addFormRef.resetFields()\r\n    },\r\n\r\n    async getUserList() {\r\n      const { data: res } = await this.$http.get('users', {\r\n        params: this.queryInfo,\r\n      })\r\n\r\n      if (res.meta.status !== 200)\r\n        return this.$message.error('获取用户列表失败')\r\n      this.userlist = res.data.users\r\n      this.total = res.data.total\r\n    },\r\n    //监听pagesize改变的事件\r\n    handleSizeChange(newSize) {\r\n      console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getUserList()\r\n    },\r\n    //监听页码值改变\r\n    handleCurrentChange(newPage) {\r\n      this.queryInfo.pagenum = newPage\r\n      this.getUserList()\r\n    },\r\n    //监听switch开关状态\r\n    async userStateChanged(userinfo) {\r\n      const { data: res } = await this.$http.put(\r\n        `users/${userinfo.id}/state/${userinfo.mg_state}`\r\n      )\r\n      if (res.meta.status !== 200) {\r\n        userinfo.mg_state = !userinfo.mg_state\r\n        return this.$message.error('更新用化状态失败')\r\n      }\r\n      this.$message.success('更新用户状态成功')\r\n      //添加进服务器\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#main {\r\n  height: 100%;\r\n\r\n  #breadCrumb {\r\n    height: 15.5px;\r\n    position: absolute;\r\n    top: 80px;\r\n  }\r\n  .el-card {\r\n    position: absolute;\r\n    top: 100px;\r\n    width: 100%-11.8px;\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}