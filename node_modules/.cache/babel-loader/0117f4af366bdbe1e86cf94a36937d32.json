{"ast":null,"code":"export default {\n  data() {\n    return {\n      //获取用户列表的参数对象\n      queryInfo: {\n        query: '',\n        pagenum: 1,\n        pagesize: 2\n      },\n      userlist: [],\n      total: 0 //数据总条数\n\n    };\n  },\n\n  created() {\n    this.getUserList();\n  },\n\n  methods: {\n    async getUserList() {\n      const {\n        data: res\n      } = await this.$http.get('users', {\n        params: this.queryInfo\n      });\n\n      if (res.meta.status !== 200) {\n        return this.$message.error(\"获取用户列表失败\");\n      }\n\n      this.userlist = res.data.user;\n      this.total = res.data.total;\n      console.log(res);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqCA;EACAA;IACA;MACA;MACAC;QACAC,SADA;QAEAC,UAFA;QAGAC;MAHA,CAFA;MAOAC,YAPA;MAQAC,QARA,CASA;;IATA;EAWA,CAbA;;EAcAC;IACA;EACA,CAhBA;;EAiBAC;IACA;MACA;QAAAR;MAAA;QAAAS;MAAA;;MAEA;QACA;MACA;;MACA;MACA;MAIAC;IACA;;EAbA;AAjBA","names":["data","queryInfo","query","pagenum","pagesize","userlist","total","created","methods","params","console"],"sourceRoot":"src/components/user","sources":["User.vue"],"sourcesContent":["<template>\r\n  <div id=\"main\">\r\n    <div id=\"breadCrumb\">\r\n      <!-- 面包屑导航 -->\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n        <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item>用户管理</el-breadcrumb-item>\r\n        <el-breadcrumb-item>用户列表</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n    <!-- 卡片视图 -->\r\n\r\n    <el-card>\r\n\r\n      <!-- 搜索与添加区域 -->\r\n\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\"\r\n          ><el-input placeholder=\"请输入内容\">\r\n            <el-button\r\n              slot=\"append\"\r\n              icon=\"el-icon-search\"\r\n            ></el-button> </el-input\r\n        ></el-col>\r\n        <el-col :span=\"4\">\r\n          <el-button type=\"primary\">添加用户</el-button>\r\n        </el-col>\r\n      </el-row>\r\n    <!-- 用户列表区